# Настройки Stepik
STEPIK_EMAIL=ваш@email.com
STEPIK_PASSWORD=ваш_пароль

# Настройки приложения
APP_PORT=8080
APP_DEBUG=false
APP_ENV=production

# Настройки базы данных (если будет)
DB_HOST=localhost
DB_PORT=5432
DB_NAME=stepik_brute
DB_USER=user
DB_PASSWORD=password

# Настройки шифрования
ENCRYPTION_KEY=сгенерируется_автоматически

# Настройки удаленного сервера
REMOTE_HOST=localhost
REMOTE_PORT=3001
REMOTE_PROTOCOL=ws
USE_REMOTE=false

# Настройки прокси (опционально)
HTTP_PROXY=http://proxy.example.com:8080
HTTPS_PROXY=https://proxy.example.com:8080
NO_PROXY=localhost,127.0.0.1

# Настройки браузера
BROWSER_TYPE=firefox
HEADLESS=false
TIMEOUT=30000
USER_AGENT=Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36

# Настройки логирования
LOG_LEVEL=INFO
LOG_FILE=/app/logs/app.log
MAX_LOG_SIZE=10MB
LOG_BACKUP_COUNT=5